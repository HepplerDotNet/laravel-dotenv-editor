{
    "name": "hepplerdotnet/laravel-dotenv-editor",
    "description": "A package for editing the .env file in your Laravel root.",
    "keywords": ["laravel", "dotenv", "dotenv-editor"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Heppler",
            "email": "patrick@heppler.net"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.9"
    },
    "autoload": {
        "psr-4": {
            "HepplerDotNet\\DotenvEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HepplerDotNet\\DotenvEditor\\DotenvEditorServiceProvider"
            ],
            "aliases": {
                "DotenvEditor": "HepplerDotNet\\DotenvEditor\\DotenvEditorFacade"
            }
        }
    }
}
